{
  "containerDefinitions": [
    {
      "memory": 300,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "name": "simple-app",
      "image": "gabalconi/itznode:latest",
      "command": [
        "nodemon",
        "server.js"
      ],
      "cpu": 10
    }
  ],
  "family": "node-webapp"
}
